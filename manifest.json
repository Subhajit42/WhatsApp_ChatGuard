{
    "manifest_version": 3,
    "name": "WhatsApp Protect",
    "version": "1.0",
    "description": "Hide Chat section of WhatsApp",

    "action": {
        "default_icon": "images/icon.png"
    },

    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": [
          "https://web.whatsapp.com/*"
        ]
      }
    ]
  }